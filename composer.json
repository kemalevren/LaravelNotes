{
    "name":        "kemalevren/laravel-notes",
    "description": "Provides the ability to add notes to your Eloquent models in Laravel By Arcanedev. I needs to laravel 10 support but his rejects. Sorry",
    "keywords":    ["kemalevren", "laravel", "notes", "noteable"],
    "homepage":    "https://github.com/kemalevren/LaravelNotes",
    "authors": [
        {
            "name":     "kemalevren",
            "email":    "kemal.evrenn@gmail.com",
            "homepage": "https://github.com/kemalevren",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "arcanedev/support": "^10.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "laravel/framework": "^10.0",
        "orchestra/testbench-core": "^7.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "kemalevren\\LaravelNotes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kemalevren\\LaravelNotes\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:dox": "phpunit --testdox"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "10.x-dev"
        },
        "laravel": {
            "providers": [
                "kemalevren\\LaravelNotes\\LaravelNotesServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
